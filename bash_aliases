alias l="ls -1 -p"
alias ll="ls -A -h -o -s"

alias cp="cp -i"
alias mv="mv -i"
alias rm="rm -i"

alias less="less -g -i -J -m -Q -u"

alias bx="bundle exec"
alias nx="npm run"

alias pgrep="pgrep -f -l"

alias mw="/mathworks/hub/bat/common/bin/mw"

alias fdns="sudo dscacheutil -flushcache; sudo killall -HUP mDNSResponder"
